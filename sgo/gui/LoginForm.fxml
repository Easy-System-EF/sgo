<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="72.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sgo.gui.LoginFormController">
   <children>
      <Label prefHeight="25.0" prefWidth="58.0" text="Senha" AnchorPane.leftAnchor="5.0" AnchorPane.topAnchor="5.0">
         <padding>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </padding>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordLoginSenha" layoutX="174.0" layoutY="13.0" prefHeight="24.0" prefWidth="58.0" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="5.0">
         <padding>
            <Insets bottom="5.0" left="5.0" top="5.0" />
         </padding>
      </PasswordField>
      <Label fx:id="labelErrorSenha" prefHeight="14.0" prefWidth="214.0" text=" " textFill="#da0606" AnchorPane.leftAnchor="200.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="btOk" mnemonicParsing="false" onAction="#onBtOkAction" prefHeight="22.0" prefWidth="58.0" text="Ok" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="35.0" />
      <Button fx:id="btCancel" layoutX="133.0" mnemonicParsing="false" onAction="#onBtCancelAction" prefHeight="22.0" prefWidth="58.0" text="Cancela" AnchorPane.leftAnchor="133.0" AnchorPane.topAnchor="35.0" />
   </children>
</AnchorPane>
